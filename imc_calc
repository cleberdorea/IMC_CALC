//Definição dos dados
let nome ="Cleber Dórea";
let peso = 84;//Em quilos
let altura = 1.79;//Em metros
let mensagem
//Fórmula
    let imc = peso/(altura*altura);
    if (imc<=30 && imc>=25){
        mensagem = "Sobrepeso";
        console.log(mensagem);
           
    }

//Apresentação do resultado
console.log(`${nome} Seu IMC é: ${imc.toFixed(1)}`);



